{
  "alias": "phpoole.phar",
  "chmod": "0755",
  "compactors": [
    "Herrera\\Box\\Compactor\\Json",
    "Herrera\\Box\\Compactor\\Php"
  ],
  "directories": [
    "config",
    "src",
    "res",
    "skeleton"
  ],
  "files": [
    "LICENSE"
  ],
  "finder": [
    {
      "name": [
        "LICENSE*",
        "License*",
        "*.php",
        "*.xml",
        "*.xsd",
        "*.yaml",
        "*.yml",
        "*.json",
        "*.txt",
        "*.twig"
      ],
      "notName": [
        "composer.json"
      ],
      "exclude": [
        "phpunit",
        "Tests",
        "tests"
      ],
      "in": [
        "vendor"
      ]
    }
  ],
  "git-version": "git_version",
  "main": "bin/phpoole",
  "output": "dist/phpoole.phar",
  "stub": true
}
