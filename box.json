{
  "alias": "cecil.phar",
  "compactors": [
    "KevinGH\\Box\\Compactor\\Json",
    "KevinGH\\Box\\Compactor\\Php"
  ],
  "finder": [
    {
      "name": [
        "*.php",
        "*.xml",
        "*.xsd",
        "*.yaml",
        "*.yml",
        "*.json",
        "*.txt",
        "*.twig",
        "LICENSE*",
        "License*",
        "VERSION"
      ],
      "notName": ["composer.json", "box.json"],
      "exclude": ["phpunit", "Tests", "tests"],
      "in": ["."]
    }
  ],
  "git-version": "git_version",
  "output": "dist/cecil.phar"
}
