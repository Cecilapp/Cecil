{
  "alias": "cecil.phar",
  "compactors": [
    "KevinGH\\Box\\Compactor\\Json",
    "KevinGH\\Box\\Compactor\\Php"
  ],
  "directories": ["config", "src", "res"],
  "files": ["LICENSE", "VERSION"],
  "finder": [
    {
      "name": [
        "LICENSE*",
        "License*",
        "*.php",
        "*.xml",
        "*.xsd",
        "*.yaml",
        "*.yml",
        "*.json",
        "*.txt",
        "*.twig",
        "VERSION"
      ],
      "notName": ["composer.json"],
      "exclude": ["phpunit", "Tests", "tests"],
      "in": ["vendor"]
    }
  ],
  "git-version": "git_version",
  "output": "dist/cecil.phar"
}
