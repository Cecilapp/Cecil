{% for page in site.pages|filter(p => p.redirect is defined)|sort_by_date %}
{{ url(page, {canonical:false}) }} {{ url(page.redirect, {canonical:false}) }} 301
{% endfor %}

{% for url, redirect in site.data.redirects %}
{{ url(url, {canonical:false}) }} {{ url(redirect, {canonical:false}) }} 301
{% endfor %}