{
  "name": "cecil/cecil",
  "description": "Your content driven static site generator.",
  "keywords": [
    "static-site-generator",
    "static-site",
    "markdown",
    "twig",
    "yaml"
  ],
  "homepage": "https://cecil.app",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Arnaud Ligny",
      "email": "arnaud@ligny.org",
      "homepage": "https://arnaudligny.fr",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/Cecilapp/Cecil",
    "issues": "https://github.com/Cecilapp/Cecil/issues"
  },
  "require": {
    "php": "^7.1",
    "cecil/phpoole": "3.0.1",
    "padraic/humbug_get_contents": "1.1.2",
    "padraic/phar-updater": "1.0.6",
    "symfony/filesystem": "4.1.7",
    "symfony/process": "4.1.7",
    "symfony/yaml": "4.1.7",
    "yosymfony/resource-watcher": "2.0.0",
    "zendframework/zend-progressbar": "2.6.0",
    "zfcampus/zf-console": "1.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.4.4",
    "phpmd/phpmd": "2.6.0",
    "humbug/box": "3.3.1"
  },
  "autoload": {
    "psr-4": {
      "Cecil\\Command\\": "src/Command",
      "Cecil\\Util\\": "src/Util"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cecil\\Test\\": "tests/"
    }
  },
  "bin": [
    "bin/cecil"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "4.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Cecilapp/PHPoole"
    }
  ],
  "scripts": {
    "test": "phpunit -c ./",
    "box": "box compile"
  }
}
