{
  "name": "phpoole/phpoole",
  "description": "PHPoole is an easy and lightweight static website generator, written in PHP.",
  "keywords": ["static-site-generator", "static-site", "flat-file", "markdown", "twig", "yaml", "phpoole"],
  "homepage": "https://phpoole.org",
  "license": "MIT",
  "type": "project",
  "authors": [
    {
      "name": "Arnaud Ligny",
      "email": "arnaud@ligny.org",
      "homepage": "http://narno.org",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/PHPoole/PHPoole",
    "issues": "https://github.com/PHPoole/PHPoole/issues"
  },
  "require": {
    "php": "^5.6 || ^7.1",
    "phpoole/library": "1.10.0",
    "padraic/humbug_get_contents": "1.0.4",
    "padraic/phar-updater": "1.0.4",
    "symfony/filesystem": "2.8.31 || 3.4.2 || 4.0.2",
    "symfony/process": "^3.3.13",
    "symfony/yaml": "2.8.31 || 3.4.2 || 4.0.2",
    "yosymfony/resource-watcher": "1.2.0",
    "zfcampus/zf-console": "1.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.26",
    "kherge/box": "2.7.5"
  },
  "autoload": {
    "psr-4" : {
      "PHPoole\\Command\\": "src/Command",
      "PHPoole\\Util\\": "src/Util"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PHPoole\\Test\\": "tests/"
    }
  },
  "bin": ["bin/phpoole"],
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/PHPoole/library"
    },
    {
      "type": "vcs",
      "url": "https://github.com/PHPoole/file_get_contents"
    }
  ]
}
